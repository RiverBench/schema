@prefix : <https://w3id.org/riverbench/schema/metadata#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix rb: <https://w3id.org/riverbench/schema/metadata#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix irao: <http://ontology.ethereal.cz/irao/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@base <https://w3id.org/riverbench/schema/metadata> .

<https://w3id.org/riverbench/schema/metadata> rdf:type owl:Ontology ;
                                               owl:versionIRI <https://w3id.org/riverbench/schema/metadata/dev> ;
                                               owl:imports <http://www.w3.org/ns/dcat> ,
                                                           <https://w3id.org/stax/1.1.0/ontology> ;
                                               dc:description "Ontology for describing datasets and profiles in the RiverBench benchmark suite."@en ;
                                               dc:title "RiverBench metadata ontology"@en ;
                                               dct:created "2023-04-30T00:00:00"^^xsd:dateTime ;
                                               dct:creator _:genid1 ;
                                               dct:issued "2023-05-05T00:00:00"^^xsd:dateTime ;
                                               dct:license <https://spdx.org/licenses/CC-BY-4.0> ;
                                               vann:preferredNamespacePrefix "rb" ;
                                               vann:preferredNamespaceUri "https://w3id.org/riverbench/schema/metadata#" ;
                                               <http://schema.org/citation> "Please cite this work according to the instructions here: https://w3id.org/riverbench/documentation/licensing/"@en .

_:genid1 foaf:homepage <https://github.com/Ostrzyciel> ,
                        <https://orcid.org/0000-0002-2543-9461> ;
          foaf:name "Piotr Sowiński" ;
          foaf:nick "Ostrzyciel" .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/description
dc:description rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
dc:title rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/created
dct:created rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/creator
dct:creator rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/issued
dct:issued rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/license
dct:license rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespacePrefix
vann:preferredNamespacePrefix rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespaceUri
vann:preferredNamespaceUri rdf:type owl:AnnotationProperty .


###  http://schema.org/citation
<http://schema.org/citation> rdf:type owl:AnnotationProperty .


###  http://xmlns.com/foaf/0.1/homepage
foaf:homepage rdf:type owl:AnnotationProperty .


###  http://xmlns.com/foaf/0.1/name
foaf:name rdf:type owl:AnnotationProperty .


###  http://xmlns.com/foaf/0.1/nick
foaf:nick rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://ontology.ethereal.cz/irao/hasFollowedProtocol
irao:hasFollowedProtocol rdf:type owl:ObjectProperty .


###  http://purl.org/dc/terms/hasPart
dct:hasPart rdfs:subPropertyOf dct:relation .


###  http://purl.org/dc/terms/isPartOf
dct:isPartOf rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf dct:relation .


###  http://purl.org/dc/terms/relation
dct:relation rdf:type owl:ObjectProperty .


###  http://www.w3.org/ns/dcat#inCatalog
dcat:inCatalog rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf dct:isPartOf ;
               rdfs:domain dcat:Resource ;
               rdfs:range dcat:Catalog .


###  http://www.w3.org/ns/dcat#resource
dcat:resource rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf dct:hasPart .


###  http://www.w3.org/ns/dcat#seriesMember
dcat:seriesMember rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf dct:relation ;
                  rdfs:comment "Indicates which datasets are included in the profile"@en ;
                  rdfs:label "Includes dataset"@en .


###  https://w3id.org/riverbench/schema/metadata#hasCategory
rb:hasCategory rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf dcat:resource ;
               rdfs:domain rb:RiverBench ;
               rdfs:range rb:Category ;
               rdfs:comment "Indicates that a benchmark category belongs to this benchmark suite."@en ;
               rdfs:label "Has benchmark category"@en .


###  https://w3id.org/riverbench/schema/metadata#hasCategoryMember
rb:hasCategoryMember rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf dct:hasPart ;
                     owl:inverseOf rb:inCategory ;
                     rdfs:domain rb:Category ;
                     rdfs:range rb:CategoryMember ;
                     rdfs:comment "For benchmark categories, indicates their members (profiles or tasks)."@en ;
                     rdfs:label "Has category member"@en .


###  https://w3id.org/riverbench/schema/metadata#hasDatasetShape
rb:hasDatasetShape rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf rb:hasShape ;
                   rdfs:domain rb:Profile ;
                   rdfs:range sh:NodeShape ;
                   rdfs:comment "Specifies the SHACL shape of distributions that are allowed in a given benchmark profile."@en ;
                   rdfs:label "Has dataset shape"@en .


###  https://w3id.org/riverbench/schema/metadata#hasDistributionShape
rb:hasDistributionShape rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf rb:hasShape ;
                        rdfs:domain rb:Profile ;
                        rdfs:range sh:NodeShape ;
                        rdfs:comment "Specifies the SHACL shape of distributions that are allowed in a given benchmark profile."@en ;
                        rdfs:label "Has distribution shape"@en .


###  https://w3id.org/riverbench/schema/metadata#hasDistributionType
rb:hasDistributionType rdf:type owl:ObjectProperty ;
                       rdfs:domain rb:Distribution ;
                       rdfs:range rb:DistributionType ;
                       rdfs:comment "Indicates the type of RiverBench dataset distribution"@en ;
                       rdfs:label "Has distribution type"@en .


###  https://w3id.org/riverbench/schema/metadata#hasProfile
rb:hasProfile rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf rb:hasCategoryMember ;
              rdfs:domain rb:Category ;
              rdfs:range rb:Profile ;
              rdfs:comment "For benchmark categories this property indicates profiles that belong to the category."@en ;
              rdfs:label "Has benchmark profile"@en .


###  https://w3id.org/riverbench/schema/metadata#hasShape
rb:hasShape rdf:type owl:ObjectProperty ;
            rdfs:range sh:NodeShape ;
            rdfs:comment "Property for specifying the desired SHACL shape of something (e.g., a dataset or a distribution)."@en ;
            rdfs:label "Has shape"@en .


###  https://w3id.org/riverbench/schema/metadata#hasStatistics
rb:hasStatistics rdf:type owl:ObjectProperty ;
                 rdfs:domain rb:StatisticsSet ;
                 rdfs:range rb:Statistics ;
                 rdfs:comment "Has a dataset statistics object"@en ;
                 rdfs:label "Has statistics"@en .


###  https://w3id.org/riverbench/schema/metadata#hasStatisticsSet
rb:hasStatisticsSet rdf:type owl:ObjectProperty ;
                    rdfs:domain rb:Distribution ;
                    rdfs:range rb:StatisticsSet ;
                    rdfs:comment "Indicates the grouping (set) of statistics for a given distribution. Multiple distributions can share one set of statistics, but a distribution must have exactly one statistics set."@en ;
                    rdfs:label "Has statistics set"@en .


###  https://w3id.org/riverbench/schema/metadata#hasStreamElementSplit
rb:hasStreamElementSplit rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf owl:topObjectProperty ;
                         rdfs:domain rb:Dataset ;
                         rdfs:range rb:StreamElementSplit ;
                         rdfs:comment "Indicates how the stream was split into elements."@en ;
                         rdfs:label "Has stream element split"@en .


###  https://w3id.org/riverbench/schema/metadata#hasSubjectShape
rb:hasSubjectShape rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf rb:hasShape ;
                   rdfs:domain rb:TopicStreamElementSplit ;
                   rdfs:range sh:NodeShape ;
                   rdfs:comment """Indicates the subject node in an RDF subject graph stream, using SHACL. The value of this property should be a SHACL sh:NodeShape with a specified target (e.g., sh:targetSubjectsOf). Only the target will be considered, restrictions on the shape will be ignored.

This property is required for RDF subject graph streams. Only sh:targetClass, sh:targetSubjectsOf, and sh:targetObjectsOf are allowed as the target specification.

This property can be specified multiple times. The different target specifications will then be treated as alternatives."""@en ;
                   rdfs:label "Has subject shape"@en .


###  https://w3id.org/riverbench/schema/metadata#hasTask
rb:hasTask rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf rb:hasCategoryMember ;
           rdfs:domain rb:Category ;
           rdfs:range rb:Task ;
           rdfs:comment "For benchmark categories this property indicates tasks that belong to the category."@en ;
           rdfs:label "Has benchmark task"@en .


###  https://w3id.org/riverbench/schema/metadata#hasTemporalProperty
rb:hasTemporalProperty rdf:type owl:ObjectProperty ;
                       rdfs:domain rb:TimeStreamElementSplit ;
                       rdfs:comment "The IRI of the property that is used in the stream to denote time at which the event occured."@en ;
                       rdfs:label "Has temporal property"@en .


###  https://w3id.org/riverbench/schema/metadata#inCategory
rb:inCategory rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf dct:isPartOf ;
              rdf:type owl:FunctionalProperty ;
              rdfs:domain rb:CategoryMember ;
              rdfs:range rb:Category ;
              rdfs:comment "Indicates that the subject (either a task or a profile) is in benchmark category. This property is functional (each task/profile must be in exactly one benchmark category)."@en ;
              rdfs:label "In benchmark category"@en .


###  https://w3id.org/riverbench/schema/metadata#isSubsetOfProfile
rb:isSubsetOfProfile rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf dct:relation ;
                     owl:inverseOf rb:isSupersetOfProfile ;
                     rdf:type owl:TransitiveProperty ;
                     rdfs:domain rb:Profile ;
                     rdfs:range rb:Profile ;
                     rdfs:comment "Indicates that this profile's datasets are all in the other profile"@en ;
                     rdfs:label "Is subset of profile"@en .


###  https://w3id.org/riverbench/schema/metadata#isSupersetOfProfile
rb:isSupersetOfProfile rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf dct:relation ;
                       rdf:type owl:TransitiveProperty ;
                       rdfs:domain rb:Profile ;
                       rdfs:range rb:Profile ;
                       rdfs:comment "Indicates that this profile contains all datasets of the other profile"@en ;
                       rdfs:label "Is superset of profile"@en .


###  https://w3id.org/riverbench/schema/metadata#targetCustom
rb:targetCustom rdf:type owl:ObjectProperty ;
                rdfs:domain sh:NodeShape ;
                rdfs:range rb:CustomShaclTarget ;
                rdfs:comment "Custom SHACL-like node target used in RiverBench for datasets where SHACL does not provide adequate means for targeting them (e.g., for RDF-star)."@en ;
                rdfs:label "Target custom"@en .


###  https://w3id.org/riverbench/schema/metadata#usesProfile
rb:usesProfile rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf rb:usesResource ;
               rdfs:range rb:Profile ;
               rdfs:comment "Indicates that the subject is using a specific RiverBench benchmark profile."@en ;
               rdfs:label "Uses benchmark profile"@en .


###  https://w3id.org/riverbench/schema/metadata#usesResource
rb:usesResource rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf dct:relation ;
                rdfs:range dcat:Resource ;
                rdfs:comment "Indicates that the subject is using the object (a dcat:Resource) in some way."@en ;
                rdfs:label "Uses resource"@en .


###  https://w3id.org/riverbench/schema/metadata#usesSystemUnderTest
rb:usesSystemUnderTest rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf rb:usesResource ;
                       rdfs:comment "Indicates that the subject is using a specific system (e.g., an RDF store)."@en ;
                       rdfs:label "Uses system under test"@en .


###  https://w3id.org/riverbench/schema/metadata#usesTask
rb:usesTask rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf rb:usesResource ;
            rdfs:range rb:Task ;
            rdfs:comment "Indicates that the subject is using a specific RiverBench benchmark task."@en ;
            rdfs:label "Uses benchmark task"@en .


###  https://w3id.org/stax/ontology#hasStreamTypeUsage
<https://w3id.org/stax/ontology#hasStreamTypeUsage> rdf:type owl:ObjectProperty .


#################################################################
#    Data properties
#################################################################

###  https://w3id.org/riverbench/schema/metadata#conformanceProperty
rb:conformanceProperty rdf:type owl:DatatypeProperty ;
                       rdfs:subPropertyOf owl:topDataProperty ;
                       rdfs:range xsd:boolean ;
                       rdfs:comment "Base property for all conformance data properties"@en ;
                       rdfs:label "Conformance property"@en .


###  https://w3id.org/riverbench/schema/metadata#conformsToRdf11
rb:conformsToRdf11 rdf:type owl:DatatypeProperty ;
                   rdfs:subPropertyOf rb:conformanceProperty ;
                   rdfs:range xsd:boolean ;
                   rdfs:comment "Whether the dataset is RDF 1.1-compliant, i.e., does not use any non-standard features, like generalized triples."@en ;
                   rdfs:label "Conforms to W3C RDF 1.1 specification"@en ;
                   rdfs:seeAlso <https://www.w3.org/TR/rdf11-concepts/> .


###  https://w3id.org/riverbench/schema/metadata#conformsToRdfStarDraft_20211217
rb:conformsToRdfStarDraft_20211217 rdf:type owl:DatatypeProperty ;
                                   rdfs:subPropertyOf rb:conformanceProperty ;
                                   rdfs:range xsd:boolean ;
                                   rdfs:comment "Whether the dataset is RDF-star compliant, i.e., does not use any non-standard features. Note that all standard RDF 1.1 datasets also qualify, as RDF-star is a superset of RDF 1.1."@en ;
                                   rdfs:label "Conforms to W3C RDF-star draft specification as of December 17, 2021"@en ;
                                   rdfs:seeAlso <https://www.w3.org/2021/12/rdf-star.html> .


###  https://w3id.org/riverbench/schema/metadata#hasFileName
rb:hasFileName rdf:type owl:DatatypeProperty ;
               rdfs:domain rb:Distribution ;
               rdfs:range xsd:string ;
               rdfs:comment "Canonical file name of this distribution"@en ;
               rdfs:label "Has file name"@en .


###  https://w3id.org/riverbench/schema/metadata#hasStreamElementCount
rb:hasStreamElementCount rdf:type owl:DatatypeProperty ;
                         rdfs:subPropertyOf owl:topDataProperty ;
                         rdfs:comment "Number of elements in the stream"@en ;
                         rdfs:label "Has stream element count"@en .


###  https://w3id.org/riverbench/schema/metadata#hasVersion
rb:hasVersion rdf:type owl:DatatypeProperty ;
              rdfs:range xsd:string ;
              rdfs:comment "Version tag of an artifact"@en ;
              rdfs:label "Has version"@en .


###  https://w3id.org/riverbench/schema/metadata#maximum
rb:maximum rdf:type owl:DatatypeProperty ;
           rdfs:subPropertyOf rb:statisticalProperty ;
           rdfs:range xsd:decimal ;
           rdfs:comment "Maximum value of a distribution"@en ;
           rdfs:label "Maximum"@en .


###  https://w3id.org/riverbench/schema/metadata#mean
rb:mean rdf:type owl:DatatypeProperty ;
        rdfs:subPropertyOf rb:statisticalProperty ;
        rdfs:range xsd:decimal ;
        rdfs:comment "Arithmetic mean of a distribution"@en ;
        rdfs:label "Mean"@en .


###  https://w3id.org/riverbench/schema/metadata#minimum
rb:minimum rdf:type owl:DatatypeProperty ;
           rdfs:subPropertyOf rb:statisticalProperty ;
           rdfs:range xsd:decimal ;
           rdfs:comment "Minimum value of a distribution"@en ;
           rdfs:label "Minimum"@en .


###  https://w3id.org/riverbench/schema/metadata#standardDeviation
rb:standardDeviation rdf:type owl:DatatypeProperty ;
                     rdfs:subPropertyOf rb:statisticalProperty ;
                     rdfs:range xsd:decimal ;
                     rdfs:comment "Standard deviation of a distribution"@en ;
                     rdfs:label "Standard deviation"@en .


###  https://w3id.org/riverbench/schema/metadata#statisticalProperty
rb:statisticalProperty rdf:type owl:DatatypeProperty ;
                       rdfs:domain rb:Statistics ;
                       rdfs:comment "The base statistical property." ;
                       rdfs:label "Statistical property"@en .


###  https://w3id.org/riverbench/schema/metadata#sum
rb:sum rdf:type owl:DatatypeProperty ;
       rdfs:subPropertyOf rb:statisticalProperty ;
       rdfs:range xsd:decimal ;
       rdfs:comment "Sum of all values in the distribution. In statistics about counts, this corresponds to the total number of given elements in the dataset."@en ;
       rdfs:label "Sum"@en .


###  https://w3id.org/riverbench/schema/metadata#uniqueCount
rb:uniqueCount rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf rb:statisticalProperty ;
               rdfs:range xsd:integer ;
               rdfs:comment "Only used for count statistics. Indicates how many unique elements are in the entire dataset. The value is estimated with a Bloom filter and is accurate to ~1%."@en ;
               rdfs:label "Unique count (estimated)"@en .


###  https://w3id.org/riverbench/schema/metadata#usesGeneralizedRdfDatasets
rb:usesGeneralizedRdfDatasets rdf:type owl:DatatypeProperty ;
                              rdfs:subPropertyOf rb:conformanceProperty ;
                              rdfs:range xsd:boolean ;
                              rdfs:comment "Whether the dataset uses the non-standard generalized datasets feature. A \"dataset\" here is used in the same meaning as in the RDF 1.1 specification."@en ;
                              rdfs:label "Uses generalized RDF datasets"@en ;
                              rdfs:seeAlso <https://www.w3.org/TR/rdf11-concepts/#section-generalized-rdf> .


###  https://w3id.org/riverbench/schema/metadata#usesGeneralizedTriples
rb:usesGeneralizedTriples rdf:type owl:DatatypeProperty ;
                          rdfs:subPropertyOf rb:conformanceProperty ;
                          rdfs:range xsd:boolean ;
                          rdfs:comment "Whether the dataset uses the non-standard generalized triples feature"@en ;
                          rdfs:label "Uses generalized triples"@en ;
                          rdfs:seeAlso "https://www.w3.org/TR/rdf11-concepts/#section-generalized-rdf"@en .


###  https://w3id.org/riverbench/schema/metadata#usesMetric
rb:usesMetric rdf:type owl:DatatypeProperty ;
              rdfs:subPropertyOf owl:topDataProperty ;
              rdfs:range xsd:string ;
              rdfs:comment "Indicates a benchmark metric that is used in a benchmark. Values of this property should be specified as the name of the metric, in the exact spelling as in the corresponding task definition. For example: \"Loading throughput\"."@en ;
              rdfs:label "Uses metric"@en .


###  https://w3id.org/riverbench/schema/metadata#usesRdfStar
rb:usesRdfStar rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf rb:conformanceProperty ;
               rdfs:range xsd:boolean ;
               rdfs:comment "Whether the dataset uses RDF-star features."@en ;
               rdfs:label "Uses RDF-star"@en ;
               rdfs:seeAlso "https://www.w3.org/2021/12/rdf-star.html"@en .


#################################################################
#    Classes
#################################################################

###  http://ontology.ethereal.cz/irao/Benchmark
irao:Benchmark rdf:type owl:Class .


###  http://ontology.ethereal.cz/irao/Dataset
irao:Dataset rdf:type owl:Class .


###  http://ontology.ethereal.cz/irao/Protocol
irao:Protocol rdf:type owl:Class .


###  http://ontology.ethereal.cz/irao/System
irao:System rdf:type owl:Class .


###  http://www.w3.org/ns/dcat#DatasetSeries
dcat:DatasetSeries rdf:type owl:Class ;
                   rdfs:subClassOf dcat:Dataset ;
                   rdfs:label "Dataset series"@en .


###  http://www.w3.org/ns/shacl#NodeShape
sh:NodeShape rdf:type owl:Class .


###  https://w3id.org/riverbench/schema/metadata#BenchmarkProtocol
rb:BenchmarkProtocol rdf:type owl:Class ;
                     rdfs:subClassOf irao:Protocol ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty rb:usesProfile ;
                                       owl:someValuesFrom rb:Profile
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty rb:usesSystemUnderTest ;
                                       owl:someValuesFrom rb:SystemUnderTest
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty rb:usesTask ;
                                       owl:someValuesFrom rb:Task
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty rb:usesMetric ;
                                       owl:someValuesFrom xsd:string
                                     ] ;
                     rdfs:comment "The parameters of a performed benchmark (rb:PerformedBenchmark). Instances of this class specify the RiverBench profile, task, systems, and metrics that were used in the benchmark."@en ;
                     rdfs:label "Benchmark protocol"@en .


###  https://w3id.org/riverbench/schema/metadata#BlankNodeCountStatistics
rb:BlankNodeCountStatistics rdf:type owl:Class ;
                            rdfs:subClassOf rb:Statistics ;
                            rdfs:comment "Statistics about the number of blank nodes in the dataset."@en ;
                            rdfs:label "Blank node count statistics"@en .


###  https://w3id.org/riverbench/schema/metadata#Category
rb:Category rdf:type owl:Class ;
            rdfs:subClassOf dcat:Resource ;
            owl:disjointWith rb:CategoryMember ;
            rdfs:comment "Class for benchmark categories in RiverBench. A category groups tasks and profiles that are compatible with each other. Each task and profile in a category has a name prefixed with the name of the category (e.g., all tasks in the \"stream\" category start with \"stream-\")."@en ;
            rdfs:label "Benchmark category"@en .


###  https://w3id.org/riverbench/schema/metadata#CategoryMember
rb:CategoryMember rdf:type owl:Class ;
                  rdfs:subClassOf dcat:Resource ;
                  rdfs:comment "Superclass for both profiles and tasks"@en ;
                  rdfs:label "Member of a benchmark category"@en .


###  https://w3id.org/riverbench/schema/metadata#CustomShaclTarget
rb:CustomShaclTarget rdf:type owl:Class ;
                     rdfs:comment "A class for all custom SHACL-like node targets in RiverBench. See also rb:targetCustom."@en ;
                     rdfs:label "Custom SHACL target"@en .


###  https://w3id.org/riverbench/schema/metadata#Dataset
rb:Dataset rdf:type owl:Class ;
           rdfs:subClassOf irao:Dataset ,
                           dcat:Dataset ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty <https://w3id.org/stax/ontology#hasStreamTypeUsage> ;
                             owl:someValuesFrom <https://w3id.org/stax/ontology#ConcreteRdfStreamType>
                           ] ;
           rdfs:comment "A dataset in the RiverBench benchmark suite"@en ;
           rdfs:label "RiverBench dataset"@en .


###  https://w3id.org/riverbench/schema/metadata#DatatypeLiteralCountStatistics
rb:DatatypeLiteralCountStatistics rdf:type owl:Class ;
                                  rdfs:subClassOf rb:Statistics ;
                                  rdfs:comment "Statistics about the number of datatype literals (NOT of type rdf:langString) in the dataset."@en ;
                                  rdfs:label "Datatype literal count statistics"@en .


###  https://w3id.org/riverbench/schema/metadata#Distribution
rb:Distribution rdf:type owl:Class ;
                rdfs:subClassOf dcat:Distribution ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty <https://w3id.org/stax/ontology#hasStreamTypeUsage> ;
                                  owl:someValuesFrom <https://w3id.org/stax/ontology#ConcreteRdfStreamType>
                                ] ;
                rdfs:comment "A distribution of a dataset in the RiverBench benchmark suite."@en ;
                rdfs:label "RiverBench dataset distribution"@en .


###  https://w3id.org/riverbench/schema/metadata#DistributionType
rb:DistributionType rdf:type owl:Class ;
                    rdfs:comment "Type of dataset distribution, indicating the corresponding streaming task formulation."@en ;
                    rdfs:label "RiverBench distribution type"@en .


###  https://w3id.org/riverbench/schema/metadata#GraphCountStatistics
rb:GraphCountStatistics rdf:type owl:Class ;
                        rdfs:subClassOf rb:Statistics ;
                        rdfs:comment "Statistics about the number of RDF graphs in the dataset, including the default graph."@en ;
                        rdfs:label "Graph count statistics"@en .


###  https://w3id.org/riverbench/schema/metadata#IriCountStatistics
rb:IriCountStatistics rdf:type owl:Class ;
                      rdfs:subClassOf rb:Statistics ;
                      rdfs:comment "Statistics about the number of IRIs in the dataset."@en ;
                      rdfs:label "IRI count statistics"@en .


###  https://w3id.org/riverbench/schema/metadata#LanguageLiteralCountStatistics
rb:LanguageLiteralCountStatistics rdf:type owl:Class ;
                                  rdfs:subClassOf rb:Statistics ;
                                  rdfs:comment "Statistics about the number of language literals in the dataset."@en ;
                                  rdfs:label "Language string count statistics"@en .


###  https://w3id.org/riverbench/schema/metadata#LiteralCountStatistics
rb:LiteralCountStatistics rdf:type owl:Class ;
                          rdfs:subClassOf rb:Statistics ;
                          rdfs:comment "Statistics about the number of literals in the dataset."@en ;
